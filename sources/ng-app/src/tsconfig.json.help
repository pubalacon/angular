{
    "compilerOptions": {
      "target": "es5", // ecmascript v5 (plus recente = v6)
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true, // creer des .map = oui, pour debugging
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [ "es2015", "dom" ],
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true,
      "typeRoots" : ["node_modules/@types"],
      "outDir": "dist"
    }
}